    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.22",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^8.0.29",
    "ts-node": "3.3.0",
